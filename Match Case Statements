n = int(input("Enter a number:"))
print("Your number is", n)

match n:
  case 0:
    print("Your number is zero")

  case 1:
    print("Your number is one")

  case 2:
    print("Your number is two")

  case 3:
    print("Your number is three")

  case _ if n >= 5 and n < 7:
    print("Your number is greater than five but less than seven")
    print(n)

  case _ if n >= 7 and n < 10:
    print("Your number is greater then seven but less than ten")
    print(n)
